---
// Import para traer los datos del cs.json al Hero
import { basics } from "cv.json";

// Variables
const { name, label, image, email, url } = basics;
const { city } = basics.location;
---

<h2>Hero</h2>

<section>
    <div class="container">
        <div class="info">
            <h1>{name}</h1>
            <h1>{label}</h1>
            <span>
                <!-- Icono -->
                {city}
            </span>
        </div>
    </div>
</section>
